{
  "expectation_suite_name": "curated_dataset",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": [
          "Name of Organisation",
          "Province",
          "Status",
          "Website URL",
          "Contact Person",
          "Contact Number",
          "Contact Email Address",
          "Fleet Size",
          "Runway Length",
          "Runway Length (m)",
          "Confidence"
        ]
      },
      "meta": {
        "notes": "Schema contract for curated dataset export."
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "Name of Organisation"
      },
      "meta": {
        "notes": "Organisation name is mandatory for every row."
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "Province",
        "value_set": [
          "Eastern Cape",
          "Free State",
          "Gauteng",
          "KwaZulu-Natal",
          "Limpopo",
          "Mpumalanga",
          "Northern Cape",
          "North West",
          "Western Cape",
          "Unknown"
        ]
      },
      "meta": {
        "notes": "Province must align with ACES canonical list or Unknown when indeterminate."
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "Status",
        "value_set": [
          "Verified",
          "Candidate",
          "Needs Review",
          "Duplicate",
          "Do Not Contact (Compliance)"
        ]
      },
      "meta": {
        "notes": "Status constrained to ACES taxonomy."
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "Website URL",
        "regex": "^(?:https://.+)?$"
      },
      "meta": {
        "notes": "Websites must resolve to HTTPS when present."
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "Contact Number",
        "regex": "^(?:\\+?27\\d{9})?$"
      },
      "meta": {
        "notes": "Phones normalised to +27 E.164 (allowing legacy rows prefixed with 27 during transition)."
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "Contact Email Address",
        "regex": "^(?:[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,})?$"
      },
      "meta": {
        "notes": "Emails must be syntactically valid when provided."
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "Confidence",
        "max_value": 100,
        "min_value": 0
      },
      "meta": {
        "notes": "Placeholder bounds; runtime enforces canonical confidence thresholds."
      }
    }
  ],
  "meta": {
    "great_expectations_version": "1.7.1",
    "notes": [
      "Contracts derived from docs/data-quality.md Phase 1.1 guidance.",
      "Validates canonical columns, province/status taxonomies, and contact hygiene."
    ]
  }
}